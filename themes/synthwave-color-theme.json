    { "name": "Dart strings", "scope": ["string.interpolated.single.dart"], "settings": { "foreground": "#f97e72" } },
    { "name": "Dart variable params", "scope": ["variable.parameter.dart"], "settings": { "foreground": "#72f1b8" } },
    { "name": "Dart numerics", "scope": ["constant.numeric.dart"], "settings": { "foreground": "#2EE2FA" } },
    { "name": "Scala variable params", "scope": ["variable.parameter.scala"], "settings": { "foreground": "#2EE2FA" } },
    { "name": "Scala", "scope": ["meta.template.expression.scala"], "settings": { "foreground": "#72f1b8" } }
