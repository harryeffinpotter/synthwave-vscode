    { "name": "C storage modifier", "scope": "storage.modifier.c", "settings": { "foreground": "#fe4450" } },
    { "name": "C++ operators", "scope": "source.cpp keyword.operator", "settings": { "foreground": "#fede5d" } },
    { "name": "C++ placeholder", "scope": "constant.other.placeholder.cpp", "settings": { "foreground": "#72f1b8", "fontStyle": "italic" } },
    { "name": "C++ include", "scope": ["keyword.control.directive.include.cpp","keyword.control.directive.define.cpp"], "settings": { "foreground": "#72f1b8" } },
    { "name": "C++ constant modifier", "scope": "storage.modifier.specifier.const.cpp", "settings": { "foreground": "#fe4450" } },
