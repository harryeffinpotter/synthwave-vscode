    { "name": "Clojure Storage", "scope": ["storage.control.clojure"], "settings": { "foreground": "#36f9f6", "fontStyle": "italic" } },
    { "name": "Clojure Metadata", "scope": ["meta.metadata.simple.clojure","meta.metadata.map.clojure"], "settings": { "foreground": "#fe4450", "fontStyle": "italic" } },
    { "name": "Clojure Macros, Quoted", "scope": ["meta.quoted-expression.clojure"], "settings": { "fontStyle": "italic" } },
    { "name": "Clojure Symbols", "scope": ["meta.symbol.clojure"], "settings": { "foreground": "#6FC3DFff" } },
    { "name": "Go basic", "scope": "source.go", "settings": { "foreground": "#6FC3DFff" } },
    { "name": "Go Function Calls", "scope": "source.go meta.function-call.go", "settings": { "foreground": "#36f9f6" } },
