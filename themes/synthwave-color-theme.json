  "tokenColors": [
    {
      "name": "Comment",
      "scope": [
        "comment",
        "string.quoted.docstring.multi.python",
        "string.quoted.docstring.multi.python punctuation.definition.string.begin.python",
        "string.quoted.docstring.multi.python punctuation.definition.string.end.python"
      ],
      "settings": { "foreground": "#9185d3", "fontStyle": "italic" }
    },

    // (left token colors largely intact; request was to replace purple BACKGROUNDS)
    {
      "name": "String",
      "scope": ["string.quoted","string.template","punctuation.definition.string"],
      "settings": { "foreground": "#6EA4FF" }
    },
    { "name": "Punctuation within templates", "scope": "string.template meta.embedded.line", "settings": { "foreground": "#b6b1b1" } },
    { "name": "Variable", "scope": ["variable","entity.name.variable"], "settings": { "foreground": "#8CD9FF" } },
    { "name": "Language variable", "scope": "variable.language", "settings": { "foreground": "#e4d0ff", "fontStyle": "bold" } },
    { "name": "Parameter", "scope": "variable.parameter", "settings": { "fontStyle": "italic" } },
    { "name": "Storage (declaration or modifier keyword)", "scope": ["storage.type","storage.modifier"], "settings": { "foreground": "#ffb6ff" } },
    { "name": "Light Blue Ref", "scope": ["unused.ref1"], "settings": { "foreground": "#87a3ff" } },
    { "name": "Light Blue Ref2", "scope": ["unused.ref2"], "settings": { "foreground": "#4effff" } },
    { "name": "Constant", "scope": "constant", "settings": { "foreground": "#f97e72" } },
    { "name": "Regex", "scope": "string.regexp", "settings": { "foreground": "#f97e72" } },
    { "name": "Number", "scope": "constant.numeric", "settings": { "foreground": "#f97e72" } },
    { "name": "Language constant (boolean, null)", "scope": "constant.language", "settings": { "foreground": "#f97e72" } },
    { "name": "Character escape", "scope": "constant.character.escape", "settings": { "foreground": "#adffd6" } },
    { "name": "Entity", "scope": "entity.name", "settings": { "foreground": "#ff89c8" } },
    { "name": "HTML or XML tag", "scope": "entity.name.tag", "settings": { "foreground": "#72f1b8" } },
    { "name": "HTML or XML tag brackets", "scope": ["punctuation.definition.tag"], "settings": { "foreground": "#36f9f6" } },
    { "name": "Tag attribute", "scope": "entity.other.attribute-name", "settings": { "foreground": "#fede5d" } },
    { "name": "Tag attribute HTML", "scope": "entity.other.attribute-name.html", "settings": { "foreground": "#fede5d", "fontStyle": "italic" } },
    { "name": "Class", "scope": ["entity.name.type","meta.attribute.class.html"], "settings": { "foreground": "#9FFFD2" } },
    { "name": "Inherited class", "scope": "entity.other.inherited-class", "settings": { "foreground": "#ffcdae" } },
    { "name": "Function", "scope": ["entity.name.function","variable.function"], "settings": { "foreground": "#ffd6d6" } },
    { "name": "JS Export", "scope": ["keyword.control.export.js","keyword.control.import.js"], "settings": { "foreground": "#72f1b8" } },
    { "name": "JS Numerics", "scope": ["constant.numeric.decimal.js"], "settings": { "foreground": "#A6F3FF" } },
    { "name": "Keyword", "scope": "keyword", "settings": { "foreground": "#7FFF00" } },
    { "name": "Control keyword", "scope": "keyword.control", "settings": { "foreground": "#9FDCFF" } },
    { "name": "Operator", "scope": "keyword.operator", "settings": { "foreground": "#b2c4ff" } },
    { "name": "Special operator", "scope": ["keyword.operator.new","keyword.operator.expression","keyword.operator.logical"], "settings": { "foreground": "#FFFFFF" } },
    { "name": "Unit", "scope": "keyword.other.unit", "settings": { "foreground": "#ffbfba" } },
    { "name": "Support", "scope": "support", "settings": { "foreground": "#ff9aa1" } },
    { "name": "Support function", "scope": "support.function", "settings": { "foreground": "#B6FFE2" } },
    { "name": "Support variable", "scope": "support.variable", "settings": { "foreground": "#1fb4ff" } },
    { "name": "Object literal key / property", "scope": ["meta.object-literal.key","support.type.property-name"], "settings": { "foreground": "#BFE9FF" } },
    { "name": "Key-value separator", "scope": "punctuation.separator.key-value", "settings": { "foreground": "#b6b1b1" } },
    { "name": "Embedded punctuation", "scope": "punctuation.section.embedded", "settings": { "foreground": "#ffec9e" } },
    { "name": "Template expression", "scope": ["punctuation.definition.template-expression.begin","punctuation.definition.template-expression.end"], "settings": { "foreground": "#72f1b8" } },
    { "name": "CSS property", "scope": ["support.type.property-name.css","support.type.property-name.json"], "settings": { "foreground": "#72f1b8" } },
    { "name": "JS Switch control", "scope": "switch-block.expr.js", "settings": { "foreground": "#72f1b8" } },
